    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "mocha": "^5.2.0",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^5.2.0",
    "node-sass": "^5.0.0",
    "prettier": "1.13.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1"
